{
  "name": "catppuccin-css",
  "version": "1.0.0",
  "description": "Minimal CSS framework with Catppuccin colors",
  "scripts": {
    "build": "npx lightningcss-cli --minify --bundle --targets \">= 0.25%\" styles.css -o styles.min.css",
    "watch": "nodemon --watch src -e css --exec \"npm run build\"",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "lightningcss": "^1.30.1",
    "nodemon": "^3.1.0"
  },
  "keywords": [
